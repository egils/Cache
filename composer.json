{
    "name": "egils/cache",
    "description": "PSR-6 compliant caching component",
    "keywords": ["cache", "psr-6"],
    "license": "MIT",
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "provide": {
        "psr/cache-implementation": "1.0.0"
    },
    "replace": {
        "psr/cache": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Psr\\Cache\\": "src/Psr/Cache",
            "Egils\\Cache\\": "src/Egils/Cache"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Egils\\Tests\\Cache\\": "tests/Egils/Tests/Cache"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability" : "dev"
}
